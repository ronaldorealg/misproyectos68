<h1>Calcular las edades de una familia</h1>
<script>

function saltarLinea() {
        document.write("<br>");    
        document.write("<br>");    
        document.write("<br>");    
    }  

    function imprimir(frase) {
        document.write(frase);
        saltarLinea();
    }    
   alert ("El programa calculara numeros del 1 al 100 ");

	var inicio = 1;
	var final = parseInt(prompt("Ingrese aqui el limite numeros"))
    alert("hay  numeros del 1 al 100" + inicio); 
	while (inicio <= final){

	imprimir("Hay 100 numeros " + inicio);
	inicio = inicio + 100
	imprimir("Fin");
    
		   } 
	

	
</script>
